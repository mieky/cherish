{
  "name": "cherish",
  "version": "0.3.2",
  "description": "A minimal cache wrapper for all kinds of function calls.",
  "author": "Mike Arvela <mike@arvela.net>",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --presets=es2015",
    "lint": "node_modules/righteous-js/lint.js",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "config": {
    "lintTargets": "src/**/*.js test/**/*.js"
  },
  "eslintConfig": {
    "extends": "./node_modules/righteous-js/.eslintrc.js",
    "env": {
      "jest": true
    }
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-jest": "^18.0.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "jest": "^18.1.0",
    "righteous-js": "^1.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mieky/cherish.git"
  },
  "bugs": {
    "url": "https://github.com/mieky/cherish/issues"
  }
}
